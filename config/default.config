buffer_size	4096

server
{
	port	8080

	host
	{
		name				Webserv
		accepted_methods	GET
		root				root
		autoindex			true
		default_index		index.html

		location */cgi-bin/*.py
		{
			route_type			cgi
			root				root
			accepted_methods	POST
		}

		location /images/*
		{
			root	root/data/
		}

		location /uploads/*
		{
			accepted_methods	POST DELETE
			allow_upload		true
			root				root/data/
		}
	}

	host
	{
		name				google
		root				root
		accepted_methods	GET

		location /googlesearch/*
		{
			route_type	redirect
			root		www.google.com
		}
	}
}
