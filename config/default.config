application
{
	listen_backlog	42
	buffer_size		1024
	mime_config		mime.config
	debug_mode		true
	debug_logging	true
	log_file		logs.log
}

server
{
	listen_port		8080

	host
	{
		name				main_server.nl main_server.com
		root				root
		methods				GET
		default_index		index.html
		default_error		error.html
		autoindex			false

		location			upload
		{
			pattern			/upload
			route			data/upload/
		}

		location			root
		{
			pattern			/images/
			route			root/data/images
		}

		location			redirect
		{
			pattern			/googlesearch*
			route			https://www.google.com
		}

		location			cgi
		{
			pattern			*.py
			route			root/cgi-bin/
		}
	}

	host
	{
		name				cgi_host
		root				root/upload
		methods				DELETE POST
		default_index		index.html
		default_error		error.html
		autoindex			false

		location			cgi
		{
			pattern			*.py
			route			root/cgi-bin/
		}
	}
}
