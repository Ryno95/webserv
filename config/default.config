buffer_size	4096

server
{
	port	8080

	host
	{
		name				Webserv
		accepted_methods	GET
		root				root

		location /cgi-bin/*.py
		{
			route_type			cgi
			root				root/cgi-bin/
			accepted_methods	POST
		}

		location /images/*
		{
			root	root/data/
		}

		location /upload/*
		{
			allow_upload	true
			root			root/data/
		}
	}

	host
	{
		name				google
		root				root
		accepted_methods	GET

		location /googlesearch/*
		{
			route_type	redirect
			root		www.google.com
		}
	}
}
