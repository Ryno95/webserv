universal
{
	listen_backlog	42
	buffer_size		1024
}

server
{
	listen_port		8080
	mime_config		mime.config

	host
	{
		name				main_server.nl main_server.com
		root				root
		methods				GET
		default_index		index.html
		default_error		error.html
		directory_listing	false
	}

	host
	{
		name				cgi_handler
		root				root/upload
		methods				DELETE POST
		default_index		index.html
		default_error		error.html
		directory_listing	false
	}
}
